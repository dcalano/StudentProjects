import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Hello world! Potato will now destroy you.");
		System.out.println("What is name of human to be DESTROYED?");
		Scanner testScanner = new Scanner(System.in);
		String name = testScanner.nextLine();
		System.out.println(name + "?");
		if (name.equals("Josh")){
			System.out.println("Good name. Horrible person. 0/10.");
		} else if (name.equals("Potato")) {
			System.out.println("ALL HAIL " + name + ", OUR LORD AND SAVIOR");
		} else {
			System.out.println("Eh, it's whatever.");
		}
		System.out.println("Well, " + name + ", how are you doing?");
		System.out.println("JUST KIDDING I DON'T CARE.");
		testScanner.close();
	}

}
